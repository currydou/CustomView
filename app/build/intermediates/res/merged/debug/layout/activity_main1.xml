<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.test.androidart.ArtActivity">

    <com.example.test.androidart.CircleView
        android:id="@+id/circleView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#3000" />

    <Button
        android:id="@+id/btn1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="AAA" />

    <Button
        android:id="@+id/btn2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="BBB" />

    <com.example.test.androidart.elasticslide.TestButton
        android:id="@+id/btnScroller"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="scroller" />
</LinearLayout>
